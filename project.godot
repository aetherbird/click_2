config_version=5

[application]

config/name="Clicker_2"
run/main_scene="res://scenes/user_interface/user_interface.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[file_customization]

folder_colors={
"res://assets/": "yellow",
"res://scenes/": "teal",
"res://scripts/": "pink"
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
